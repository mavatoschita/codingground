void afficheTab (float tableau[], int Taille) {
   int i;
   int j; //index
    for (i = 0; i < Taille; i++ ){
    for (j = 0; j < Taille; j++ )
   {
      printf("tableau[%d][%d] = %1.2f%\n", i,j, tableau[i][j] );
         }
}

